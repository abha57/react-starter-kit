{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "concurrently \"rm -rf dist\" \"webpack --mode development --config webpack-client.config.js\" \"webpack --mode development --config webpack-server.config.js\"",
    "buildDev": "concurrently \"rm -rf dist\" \"webpack --mode development --config webpack-client.config.js --watch\" \"webpack --mode development --config webpack-server.config.js --watch\"",
    "start": "nodemon --inspect=7000 ./dist/server.js",
    "startClient": "webpack-dev-server --mode development --config webpack-client.config.js --open"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "concurrently": "^5.3.0",
    "connect-history-api-fallback": "^1.6.0",
    "css-loader": "^3.5.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.9.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "sass-loader": "^8.0.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.8.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-node-externals": "^2.5.1"
  }
}
